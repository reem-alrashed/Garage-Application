<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.project.ui.setting.SettingFragment"
        android:label="الإعدادات"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.project.ui.dashboard.DashboardFragment"
        android:label="الرئيسية"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.project.ui.profile.ProfileFragment"
        android:label="الملف الشخصي"
        tools:layout="@layout/fragment_profile" />
